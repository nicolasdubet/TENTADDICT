<h1> Create new tent</h1>


<%= form_for Tent.new do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :number_of_couch %>
  <%= f.text_field :number_of_couch %> 
  <%= f.label :season %>
  <%= f.text_field :season %>
  <%= f.label :weight %>
  <%= f.text_field :weight %>
  <%= f.submit %>
<% end %>